  "scripts": {
    "build": "prisma generate && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start ",
    "start:dev": "nest start --watch -b swc",
    "start:dev:local": "dotenv -e .env.local nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:prod:local": "dotenv -e .env.local node dist/main",
    "start:migrate:prod": "pnpm prisma migrate deploy && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:generate:local": "dotenv -e .env.local prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:local": "dotenv -e .env.local prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:studio:local": "dotenv -e .env.local prisma studio",
    "prisma:seed": "ts-node prisma/seed.ts"
  },
